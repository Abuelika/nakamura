{
  "sakai:query-language": "xpath",
  "sakai:query-template": "//*[(@sling:resourceType='sakai/file' and (jcr:contains(., '{search}') or jcr:contains(jcr:content, '{search}'))) and @sakai:user!='{_me}'{_mycontacts}{_tags}]{_order}",
  "sling:resourceType": "sakai/search",
  "sakai:batchresultprocessor": "Files",
  "sakai:propertyprovider": "Files"
}
