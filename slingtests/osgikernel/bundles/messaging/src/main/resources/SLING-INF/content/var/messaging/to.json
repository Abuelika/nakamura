{
  "sakai:query-language": "xpath",
  "sakai:query-template": "{_userPrivatePath}/message//element(*)Metadata[@sling:resourceType='sakai/message' and @sakai:type='message' and jcr:contains(@sakai:to, '*{to}*')] order by @{sortOn} {sortOrder}",
