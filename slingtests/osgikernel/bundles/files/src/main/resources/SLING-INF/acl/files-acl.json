{

 "principals" : [
    { 
    	"name":"owner", 
    	"isgroup":"true",
    	"members":[],
    	"dynamic":"true"
    },
    { 
    	"name":"everyone", 
    	"isgroup":"true",
    	"members":[],
    	"dynamic":"true"
    }
 ],
 "acl" : [
 	{
 		"path":"/_user/files",
 		"principal":"owner",
 		"privilege@jcr:read":"granted",
 		"privilege@jcr:write":"granted"
 		
 	},
 	{
 		"path":"/_user/files/incoming",
 		"principal":"everyone",
 		"privilege@jcr:read":"granted",
 		"privilege@jcr:write":"granted"
 		
 	},
 	{
 		"path":"/_user/public",
 		"principal":"everyone",
 		"privilege@jcr:read":"granted"
 	}
 ],

"privileges" : [
	"jcr:read",
	"jcr:modifyProperties",
	"jcr:addChildNodes",
	"jcr:removeNode",
	"jcr:removeChildNodes",
	"jcr:write",
	"jcr:readAccessControl",
	"jcr:modifyAccessControl",
	"jcr:all"
]
 
}