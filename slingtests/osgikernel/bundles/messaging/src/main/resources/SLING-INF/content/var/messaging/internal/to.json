{
  "sakai:query-language": "xpath",
  "sakai:query-template": "/jcr:root{_userPrivatePath}//element(*)Metadata[@sling:resourceType='sakai/message' and @sakai:type='internal' and jcr:contains(@sakai:to, '*{to}*')] order by @{sortOn|jcr:created} {sortOrder|descending}",
