{
  "sakai:query-language": "xpath",
  "sakai:query-template": "//*[(@sling:resourceType='sakai/file' and @sakai:user='{_me}' and (jcr:contains(., '{search}') or jcr:contains(jcr:content, '{search}'))){_usedin}{_tags}]{_order}",
  "sling:resourceType": "sakai/search",
  "sakai:batchresultprocessor": "Files",
  "sakai:propertyprovider": "Files"
}
