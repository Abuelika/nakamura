{
  "sakai:query-language": "xpath",
  "sakai:query-template": "/{_connectionstore}//*[@sling:resourceType='sakai/contact' and @sakai:state='ACCEPTED']/jcr:deref(@jcr:reference, '*')[jcr:contains(@firstName, '*{s}*') or jcr:contains(@lastName, '*{s}*') or jcr:contains(@email, '*{s}*')]",
  "sling:resourceType": "sakai/search",
  "sakai:propertyprovider" : "Connection",
  "sakai:processor" : "ConnectionFinder"
}